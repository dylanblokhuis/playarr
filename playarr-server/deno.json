{
  "imports": {
    "std/": "https://deno.land/std@0.174.0/",
    "oak": "https://deno.land/x/oak@v11.1.0/mod.ts",
    "sqlite": "https://deno.land/x/sqlite3@0.7.3/mod.ts",
    "kysely": "npm:kysely@0.23.4"
  },
  "tasks": {
    "dev": "deno run --unstable --allow-read --allow-net --allow-env --allow-ffi --allow-write --watch app.ts",
    "migrate": "deno run --allow-env --allow-read --allow-ffi --unstable tasks/migrate.ts",
    "create_migration": "deno run --allow-read --allow-write tasks/create_migration.ts",
    "create_package": "deno run --allow-read --allow-write --allow-run tasks/create_package.ts"
  }
}